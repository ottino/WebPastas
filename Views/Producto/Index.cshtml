@model List<WebPastas.Models.ViewModels.ListProductoViewModel>

@{
    ViewBag.Title = "Productos";
}

<h2>@ViewBag.Title</h2>

<div class="row" style="text-align:right;padding:15px">
    <a class="btn btn-primary">Nuevo</a>
</div>

<div class="row">
    <div class="col-md-12">
        <table class="table">
            <tr>
                <th>Codigo</th> 
                <th>Nombre</th> 
                <th>#</th> 
            </tr>
            @foreach (var oElemento in Model)
            {
                <tr>
                    <td>@oElemento.id</td>
                    <td>@oElemento.Descripcion</td>
                    <td>
                        <a class="btn btn-default">Editar</a>
                        <a class="btn btn-default">Eliminar</a>
                    </td>
                </tr>

            }
        </table>
    </div>
</div>

